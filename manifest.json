{
    "name": "Clean bookmarks",
    "version": "1.0",
    "description": "This will help you finish reading all of your bookmarks and keeping them clean",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "permissions": ["storage"],
    "page_action": {
        "default_popup": "popup.html"
      },
    "manifest_version": 2
  }