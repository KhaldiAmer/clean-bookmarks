{
    "name": "Clean bookmarks",
    "version": "1.0",
    "description": "This will help you finish reading all of your bookmarks and keeping them clean",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "permissions": ["storage", "declarativeContent"],
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
          }
    },
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      },
    "manifest_version": 2
  }